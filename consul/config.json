{
  "service": {
    "Name": "stockmanagement",
    "Id": "stockmanagement-service",
    "Port": 8080,
    "Tags": ["openlmis-service"],
    "check": {
      "interval": "10s",
      "http": "http://HOST:PORT/actuator/health",
      "timeout": "5s",
      "deregisterCriticalServiceAfter": "10m"
    }
  },
  "raml": "/consul/api-definition.yaml",
  "path": [
    "stockmanagement",
    "stockmanagement/docs",
    "stockmanagement/docs/<all>",
    "stockmanagement/webjars",
    "stockmanagement/webjars/<all>"
  ]
}
